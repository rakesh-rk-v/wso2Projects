<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from BSCS_ServiceService WSDL service -->

  <service name="BSCS_Service" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://service.bscs.com"/>
      <parameter name="wsdlServiceElement" value="BSCS_ServiceService"/>
      <parameter name="schemaQualified" value="http://service.bscs.com,http://model.bscs.com"/>
      <parameter name="wsdlServicePort" value="BSCS_Service"/>
      <parameter name="className" value="com.bscs.service.BSCS_ServiceSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="BSCS_Service"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="getDataFromDB" qname="operNS:getDataFromDB" xmlns:operNS="http://service.bscs.com" returnQName="retNS:getDataFromDBReturn" xmlns:retNS="http://service.bscs.com" returnType="rtns:TestDataModel" xmlns:rtns="http://model.bscs.com" soapAction="" >
      </operation>
      <parameter name="allowedMethods" value="getDataFromDB"/>

      <typeMapping
        xmlns:ns="http://model.bscs.com"
        qname="ns:TestDataModel"
        type="java:com.bscs.model.TestDataModel"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
  </service>
</deployment>
