<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from Endpoint_USERService WSDL service -->

  <service name="Endpoint_USER" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://user.knot.com"/>
      <parameter name="wsdlServiceElement" value="Endpoint_USERService"/>
      <parameter name="schemaQualified" value="http://user.knot.com"/>
      <parameter name="wsdlServicePort" value="Endpoint_USER"/>
      <parameter name="className" value="com.knot.user.Endpoint_USERSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="Endpoint_USER"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="returnString" qname="operNS:returnString" xmlns:operNS="http://user.knot.com" returnQName="retNS:returnStringReturn" xmlns:retNS="http://user.knot.com" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:name" xmlns:pns="http://user.knot.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="returnString"/>

  </service>
</deployment>
