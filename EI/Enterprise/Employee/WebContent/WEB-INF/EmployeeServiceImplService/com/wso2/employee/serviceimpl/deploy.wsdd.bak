<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from EmployeeServiceImplService WSDL service -->

  <service name="EmployeeServiceImpl" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://serviceimpl.employee.wso2.com"/>
      <parameter name="wsdlServiceElement" value="EmployeeServiceImplService"/>
      <parameter name="schemaQualified" value="http://vivo.employee.wso2.com,http://serviceimpl.employee.wso2.com"/>
      <parameter name="wsdlServicePort" value="EmployeeServiceImpl"/>
      <parameter name="className" value="com.wso2.employee.serviceimpl.EmployeeServiceImplSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="EmployeeServiceImpl"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="getDetails" qname="operNS:getDetails" xmlns:operNS="http://serviceimpl.employee.wso2.com" returnQName="retNS:getDetailsReturn" xmlns:retNS="http://serviceimpl.employee.wso2.com" returnType="rtns:Employee" xmlns:rtns="http://vivo.employee.wso2.com" soapAction="" >
        <parameter qname="pns:id" xmlns:pns="http://serviceimpl.employee.wso2.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="insert" qname="operNS:insert" xmlns:operNS="http://serviceimpl.employee.wso2.com" returnQName="retNS:insertReturn" xmlns:retNS="http://serviceimpl.employee.wso2.com" returnType="rtns:Employee" xmlns:rtns="http://vivo.employee.wso2.com" soapAction="" >
        <parameter qname="pns:emp" xmlns:pns="http://serviceimpl.employee.wso2.com" type="tns:Employee" xmlns:tns="http://vivo.employee.wso2.com"/>
      </operation>
      <operation name="delete" qname="operNS:delete" xmlns:operNS="http://serviceimpl.employee.wso2.com" returnQName="retNS:deleteReturn" xmlns:retNS="http://serviceimpl.employee.wso2.com" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:id" xmlns:pns="http://serviceimpl.employee.wso2.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="insert getDetails delete"/>

      <typeMapping
        xmlns:ns="http://vivo.employee.wso2.com"
        qname="ns:Employee"
        type="java:com.wso2.employee.vivo.Employee"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
  </service>
</deployment>
