<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from StudentService WSDL service -->

  <service name="Student" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://student.knot.com"/>
      <parameter name="wsdlServiceElement" value="StudentService"/>
      <parameter name="schemaQualified" value="http://student.knot.com"/>
      <parameter name="wsdlServicePort" value="Student"/>
      <parameter name="className" value="com.knot.student.StudentSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="Student"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="get" qname="operNS:get" xmlns:operNS="http://student.knot.com" returnQName="retNS:getReturn" xmlns:retNS="http://student.knot.com" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:id" xmlns:pns="http://student.knot.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="get"/>

  </service>
</deployment>
