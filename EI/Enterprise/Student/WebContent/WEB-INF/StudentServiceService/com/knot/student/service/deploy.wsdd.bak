<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from StudentServiceService WSDL service -->

  <service name="StudentService" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://service.student.knot.com"/>
      <parameter name="wsdlServiceElement" value="StudentServiceService"/>
      <parameter name="schemaQualified" value="http://service.student.knot.com,http://student.knot.com"/>
      <parameter name="wsdlServicePort" value="StudentService"/>
      <parameter name="className" value="com.knot.student.service.StudentServiceSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="StudentService"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="insert" qname="operNS:insert" xmlns:operNS="http://service.student.knot.com" returnQName="retNS:insertReturn" xmlns:retNS="http://service.student.knot.com" returnType="rtns:Student" xmlns:rtns="http://student.knot.com" soapAction="" >
        <parameter qname="pns:std" xmlns:pns="http://service.student.knot.com" type="tns:Student" xmlns:tns="http://student.knot.com"/>
      </operation>
      <operation name="getStudents" qname="operNS:getStudents" xmlns:operNS="http://service.student.knot.com" returnQName="retNS:getStudentsReturn" xmlns:retNS="http://service.student.knot.com" returnType="rtns:Student" xmlns:rtns="http://student.knot.com" soapAction="" >
        <parameter qname="pns:id" xmlns:pns="http://service.student.knot.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="getStudents insert"/>

      <typeMapping
        xmlns:ns="http://student.knot.com"
        qname="ns:Student"
        type="java:com.knot.student.Student"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
  </service>
</deployment>
