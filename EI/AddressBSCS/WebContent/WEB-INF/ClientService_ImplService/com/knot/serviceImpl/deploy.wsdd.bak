<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from ClientService_ImplService WSDL service -->

  <service name="ClientService_Impl" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://serviceImpl.knot.com"/>
      <parameter name="wsdlServiceElement" value="ClientService_ImplService"/>
      <parameter name="schemaQualified" value="http://model.knot.com,http://serviceImpl.knot.com"/>
      <parameter name="wsdlServicePort" value="ClientService_Impl"/>
      <parameter name="className" value="com.knot.serviceImpl.ClientService_ImplSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="ClientService_Impl"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="insertClient_Adress" qname="operNS:insertClient_Adress" xmlns:operNS="http://serviceImpl.knot.com" returnQName="retNS:insertClient_AdressReturn" xmlns:retNS="http://serviceImpl.knot.com" returnType="rtns:Adress" xmlns:rtns="http://model.knot.com" soapAction="" >
        <parameter qname="pns:add" xmlns:pns="http://serviceImpl.knot.com" type="tns:Adress" xmlns:tns="http://model.knot.com"/>
      </operation>
      <operation name="getNewAdress" qname="operNS:getNewAdress" xmlns:operNS="http://serviceImpl.knot.com" returnQName="retNS:getNewAdressReturn" xmlns:retNS="http://serviceImpl.knot.com" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
      </operation>
      <parameter name="allowedMethods" value="getNewAdress insertClient_Adress"/>

      <typeMapping
        xmlns:ns="http://model.knot.com"
        qname="ns:Adress"
        type="java:com.knot.model.Adress"
        serializer="org.apache.axis.encoding.ser.BeanSerializerFactory"
        deserializer="org.apache.axis.encoding.ser.BeanDeserializerFactory"
        encodingStyle=""
      />
  </service>
</deployment>
