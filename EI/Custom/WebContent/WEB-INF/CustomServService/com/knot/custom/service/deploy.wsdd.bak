<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from CustomServService WSDL service -->

  <service name="CustomServ" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://service.custom.knot.com"/>
      <parameter name="wsdlServiceElement" value="CustomServService"/>
      <parameter name="schemaQualified" value="http://service.custom.knot.com"/>
      <parameter name="wsdlServicePort" value="CustomServ"/>
      <parameter name="className" value="com.knot.custom.service.CustomServSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="CustomServ"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="getNameById" qname="operNS:getNameById" xmlns:operNS="http://service.custom.knot.com" returnQName="retNS:getNameByIdReturn" xmlns:retNS="http://service.custom.knot.com" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:id" xmlns:pns="http://service.custom.knot.com" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="getNameById"/>

  </service>
</deployment>
